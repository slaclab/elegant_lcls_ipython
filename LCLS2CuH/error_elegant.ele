
&run_setup
  semaphore_file = run_setup.semaphore,
  lattice = "elegant.lte",
  output = "run_setup.output.sdds",
  p_central = 264.186,
  random_number_seed = 9876543210,
  use_beamline = <lattice_name>,
  show_element_timing = 1
&end

