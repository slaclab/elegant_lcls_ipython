  item = "N_KICKS",
  type = "RFCW",
  value = 0,
&end

&alter_elements
  name = *,
  item = "LSC",
  type = "RFCW",
  value = 0,
&end

&alter_elements
  name = *,
  item = "LSC",
  type = "LSCDRIFT",
  value = 0,
&end

&alter_elements
  name = *,
  item = "LINEARIZE",
  type = "CSRCSBEND",
  value = 1,
&end

&alter_elements
  name = *,
  item = "ISR",
  type = "CSRCSBEND",
  value = 0,
&end

&alter_elements
  name = *,
  item = "CSR",
  type = "CSRCSBEND",
  value = 0,
&end

&alter_elements
  name = *,
  item = "CSR",
  type = "CSRDRIFT",
  value = 0,
&end

&run_control
  n_steps = <iter>,
  first_is_fiducial = 1,
  reset_rf_for_each_step = 0
&end

!!!!!!!!!!!!!!
! Links
!!!!!!!!!!!!!!

! Link is not needed for error study
! error_element can control linking on its own

!&link_elements
  source = "L1_9_50",
  target = "L1_*",
  item = "PHASE",
  mode = "dynamic",
  equation = " 1 *"
!&end

!!!!!!!!!!!!!!
! Errors
!!!!!!!!!!!!!!

&error_control
  no_errors_for_first_step = 1,
  error_log = "error.log"
&end

&error_element
  name = "L1_*",
  item = "PHASE"
  amplitude = <phase_error>,
  fractional = 0,
  bind_across_names = 1
&end

